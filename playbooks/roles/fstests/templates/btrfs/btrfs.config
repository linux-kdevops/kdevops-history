MKFS_OPTIONS='-f -K -O no-holes -f -R ^free-space-tree'
SCRATCH_DEV_POOL="{{ fstests_scratch_devpool }}"
MOUNT_OPTIONS="-o compress=zlib,discard=async""
TEST_FS_MOUNT_OPTS="-o compress=zlib,discard=async""
{% endif %}

